name: CI/CD

on:
  push:
  pull_request:
    branches:
      - master

jobs:
  build:

    runs-on: ubuntu-latest

    steps:
      - uses: actions/checkout@v3
      - name: Set up Python 3.x
        uses: actions/setup-python@v4
        with:
          python-version: '3.9'

 deploy:
   runs-on: ubuntu-latest
   steps:
     - name: Print hello world
       run: python -c "print('Hello, World!')"
